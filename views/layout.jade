doctype html

html
  head
    title= title
    link(rel='stylesheet', href='https://unpkg.com/purecss@1.0.0/build/pure-min.css')
    link(rel='stylesheet', href='https://unpkg.com/purecss@1.0.0/build/grids-responsive-min.css')
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Raleway:200')
    link(rel='stylesheet', href='https://purecss.io/combo/1.18.13?/css/main-grid.css&/css/main.css&/css/rainbow/baby-blue.css')
  body
    div#layout
      div#menu
        a(href='/').pure-menu-heading UNCC ACM
        ul.pure-menu-list
          each qName in questionNames
            li.pure-menu-item
              a(href='/question/' + qName).pure-menu-link=qName
      div#main
        block content
